<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class lazy="false" name="com.vaccum.entity.Order" table="user_order">
  <id column="order_id" name="orderId">
   <generator class="native"/>
  </id>
  <property generated="never" lazy="false" name="address" type="java.lang.String">
   <column name="order_address"/>
   </property>
   <property generated="never" lazy="false" name="bproductPic" type="java.lang.String">
   <column name="bproduct_pic"/>
  </property>
  <property generated="never" lazy="false" name="bproductName" type="java.lang.String">
   <column name="bproduct_name"/>
  </property>
  <property generated="never" lazy="false" name="buyerName" type="java.lang.String">
   <column name="buyer_name"/>
  </property>
  <property generated="never" lazy="false" name="sellerName" type="java.lang.String">
   <column name="seller_name"/>
  </property>
  <property generated="never" lazy="false" name="sproductPic" type="java.lang.String">
   <column name="sproduct_pic"/>
  </property>
  <property generated="never" lazy="false" name="sproductName" type="java.lang.String">
   <column name="sproduct_name"/>
  </property>
  <property generated="never" lazy="false" name="buyerPhone" type="java.lang.String">
   <column name="buyer_phone"/>
  </property>
  <property generated="never" lazy="false" name="sellerPhone" type="java.lang.String">
   <column name="seller_phone"/>
  </property>
  <property generated="never" lazy="false" name="status" type="java.lang.Integer">
   <column name="status"/>
  </property>
  <property generated="never" lazy="false" name="bproductId" type="java.lang.Integer">
   <column name="bproduct_id"/>
  </property>
  <property generated="never" lazy="false" name="sproductId" type="java.lang.Integer">
   <column name="sproduct_id"/>
  </property>
 </class>
</hibernate-mapping>
