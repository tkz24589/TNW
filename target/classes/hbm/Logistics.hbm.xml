<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class lazy="false" name="com.vaccum.entity.LogisticsDocument" table="logistics">
  <id column="logistics_id" name="logisticsId">
   <generator class="native"/>
  </id>
  <property generated="never" lazy="false" name="QRPic" type="java.lang.String">
   <column name="qrpic"/>
  </property>
  <property generated="never" lazy="false" name="orderId" type="java.lang.Integer">
   <column name="order_id"/>
  </property>
 </class>
</hibernate-mapping>
