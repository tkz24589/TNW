<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class lazy="false" name="com.vaccum.entity.ProductRequest" table="product_request">
  <id column="request_id" name="requestId">
   <generator class="native"/>
  </id>
  <property generated="never" lazy="false" name="buyerId" type="java.lang.Integer">
   <column length="30" name="buyer_id"/>
  </property>
  <property generated="never" lazy="false" name="sproductId" type="java.lang.Integer">
   <column name="sproduct_id"/>
  </property>
  <property generated="never" lazy="false" name="bproductId" type="java.lang.Integer">
   <column name="bproduct_id"/>
  </property>
  <property generated="never" lazy="false" name="sellerId" type="java.lang.Integer">
   <column name="seller_id"/>
  </property>
 </class>
</hibernate-mapping>
