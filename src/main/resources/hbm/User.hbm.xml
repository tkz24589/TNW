<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class lazy="false" name="com.vaccum.entity.User" table="user">
  <id column="user_id" name="userId">
   <generator class="native"/>
  </id>
  <property generated="never" lazy="false" name="userName" type="java.lang.String">
   <column length="30" name="user_name"/>
  </property>
  <property generated="never" lazy="false" name="passWord" type="java.lang.String">
   <column name="user_password"/>
  </property>
  <property generated="never" lazy="false" name="type" type="java.lang.String">
   <column name="user_type"/>
  </property>
  <property generated="never" lazy="false" name="address" type="java.lang.String">
   <column name="user_address"/>
  </property>
  <property generated="never" lazy="false" name="userPhone" type="java.lang.String">
   <column name="user_phone"/>
  </property>
  <property generated="never" lazy="false" name="userPhoto" type="java.lang.String">
   <column name="user_photo"/>
  </property>
 </class>
</hibernate-mapping>
